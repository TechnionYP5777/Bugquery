<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="BugQuery Commands"
            id="Window.commands.bugquery">
      </category>
      <command
            name="Get Stack from Console"
            categoryId="Window.commands.bugquery"
            id="Window.commands.bugquery.fromconsole">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="Window.commands.bugquery.fromconsole"
            class="bugquery.actions.FromConsole">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="Window.commands.bugquery.fromconsole"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="BugQuery"
               mnemonic="B"
               id="Window.menus.bugQuery">
            <command
                  commandId="Window.commands.bugquery.fromconsole"
                  mnemonic="S"
                  id="Window.menus.bugQuery">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="Window.toolbars.bugQueryToolbar">
            <command
                  commandId="Window.commands.bugquery.fromconsole"
                  icon="icons/bugquery.gif"
                  tooltip="Scrap data from console"
                  id="Window.toolbars.bugQueryToolbar">
            </command>
         </toolbar>
      </menuContribution>
   </extension>

</plugin>
