<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.commands">
      <category name="BugQuery Commands" id="bugquery.commands" />
      <command name="Get Trace from Console" categoryId="bugquery.commands" id="bugquery.commands.fromconsole" />
      <command name="Get Trace from Clipboard" categoryId="bugquery.commands" id="bugquery.commands.fromclipboard" />
      <command name="Get Trace from Input Window" categoryId="bugquery.commands" id="bugquery.commands.frominputdialog" />
      <command defaultHandler="bugquery.actions.FromConsole" categoryId="bugquery.commands" id="bugquery.commands.dropdownmenu" name="Get Trace From Console" />
   </extension>
   <extension point="org.eclipse.ui.handlers">
      <handler commandId="bugquery.commands.fromconsole" class="bugquery.actions.FromConsole" />
      <handler commandId="bugquery.commands.fromclipboard" class="bugquery.actions.FromClipboard" />
      <handler commandId="bugquery.commands.frominputdialog" class="bugquery.actions.FromInputDialog" />
   </extension>
   <extension point="org.eclipse.ui.bindings">
      <key commandId="bugquery.commands.fromconsole" contextId="org.eclipse.ui.contexts.window" sequence="M1+6" schemeId="org.eclipse.ui.defaultAcceleratorConfiguration" />
   </extension>
   <extension point="org.eclipse.ui.menus">
      <menuContribution locationURI="menu:bugquery.commands.dropdownmenu">
         <command commandId="bugquery.commands.fromconsole" label="Trace from Console" style="push" />
         <command commandId="bugquery.commands.frominputdialog" label="Trace from User Dialog" style="push" />
         <command commandId="bugquery.commands.fromclipboard" label="Trace from Clipboard" style="push" />
      </menuContribution>
      <menuContribution locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar id="toolbarmenu">
            <command commandId="bugquery.commands.dropdownmenu" icon="icons/bugquery.gif" label="BugQuery Toolbar" style="pulldown" />
         </toolbar>
      </menuContribution>
   </extension>
</plugin>